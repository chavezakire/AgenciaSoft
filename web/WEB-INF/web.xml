<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <display-name>AgenciaSoft</display-name>
    <servlet>
        <servlet-name>ServletPersona</servlet-name>
        <servlet-class>src.ServletPersona</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletHabitacion</servlet-name>
        <servlet-class>src.ServletHabitacion</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletReserva</servlet-name>
        <servlet-class>src.ServletReserva</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletPersona</servlet-name>
        <url-pattern>/ServletPersona</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletHabitacion</servlet-name>
        <url-pattern>/ServletHabitacion</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletReserva</servlet-name>
        <url-pattern>/ServletReserva</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
